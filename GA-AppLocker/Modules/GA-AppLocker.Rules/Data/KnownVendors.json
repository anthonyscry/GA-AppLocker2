  "_PathCategories": {
    "Windows": {
      "Patterns": ["C:\\Windows\\*", "%WINDIR%\\*", "%SYSTEMROOT%\\*"],
      "RiskLevel": "Low",
      "Description": "Windows system files"
    },
    "ProgramFiles": {
      "Patterns": ["C:\\Program Files\\*", "C:\\Program Files (x86)\\*", "%PROGRAMFILES%\\*", "%PROGRAMFILES(X86)%\\*"],
      "RiskLevel": "Low",
      "Description": "Standard program installations"
    },
    "UserInstalled": {
      "Patterns": ["*\\Users\\*\\AppData\\*", "*\\AppData\\Local\\*", "*\\AppData\\Roaming\\*"],
      "RiskLevel": "Medium",
      "Description": "User-installed applications"
    },
    "Temp": {
      "Patterns": ["*\\Temp\\*", "*\\tmp\\*", "%TEMP%\\*"],
      "RiskLevel": "High",
      "Description": "Temporary directories"
    },
    "Downloads": {
      "Patterns": ["*\\Downloads\\*", "*\\Desktop\\*"],
